<doctrine-mongo-mapping xmlns="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping
                http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping.xsd">

    <document name="Cubiche\Domain\Persistence\Tests\Fixtures\User" collection="user">
        <field name="id" id="true" type="Identifier" strategy="none" target="Cubiche\Domain\Persistence\Tests\Fixtures\UserId"/>
        <field fieldName="name" name="name" type="string" />
        <field fieldName="age" name="age" type="integer" />
        <field fieldName="phonenumbers" name="phonenumbers" type="ArrayCollection" innerType="Phonenumber"/>
    </document>
    
</doctrine-mongo-mapping>