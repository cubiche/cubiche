{
    "name": "cubiche/cubiche",
    "type": "project",
    "description": "Just another DDD solution in PHP",
    "keywords": [
        "ddd",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Karel Osorio",
            "email": "osorioramirez@gmail.com"
        },
        {
            "name": "Ivannis SuÃ¡rez",
            "email": "ivannis.suarez@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "myclabs/php-enum": "^1.4",
        "doctrine/mongodb-odm": "^1.0.5",
        "ramsey/uuid": "^3.2",
        "symfony/property-access": "^3.0"
    },
    "require-dev": {
        "atoum/atoum": "~2.5",
        "atoum/stubs": "~2.5",
        "atoum/visibility-extension": "~1.0",
        "phpunit/phpunit": "~4.2",
        "cubiche/cqt": "dev-master"
    },
    "autoload": {
        "exclude-from-classmap": [
            "src/**/Tests/"
        ],
        "psr-0": {
            "Cubiche\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "Cubiche\\Tools\\CodeQualityTool::checkHooks"
        ],
        "post-update-cmd": [
            "Cubiche\\Tools\\CodeQualityTool::checkHooks"
        ]
    }
}
